{
  "name": "dcodegroup/laravel-configuration",
  "description": "Simple package which dcode uses to handle application configuration and setting storage from third parties",
  "keywords": [
    "laravel",
    "dcode",
    "settings",
    "configuration"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Nathan Rzepecki",
      "email": "nathan@lionslair.net.au",
      "homepage": "https://nathanrzepecki.me",
      "role": "Developer"
    },
    {
      "name": "Dcode Group",
      "email": "forge@dcodegroup.com",
      "homepage": "https://dcodegroup.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "laravel/framework": "^7.0|^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "laravel/pint": "^0.2.3"
  },
  "autoload": {
    "psr-4": {
      "Dcodegroup\\LaravelConfiguration\\": "src"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dcodegroup\\LaravelConfiguration\\LaravelConfigurationServiceProvider"
      ]
    }
  }
}
